<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <desc
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Description"
                        name="./desc"
                        xtype="textarea"/>
                    <targeturl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Target Url"
                        name="./targeturl"
                        xtype="pathfield"/>
                    <imgpath
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Image Path"
                        name="./imgpath"
                        xtype="pathfield"/>
                    <imgtarget
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Image Target Url"
                        name="./imgtarget"
                        xtype="pathfield"/>
                    <targettext
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Target Text"
                        name="./targettext"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
